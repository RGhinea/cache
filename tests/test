$cache = new RedisCache($redis);
$cache->setNamespace('dt');

$tags = array();
for ($i=4; $i<=4; $i++) {
    $tags[$i]= base_convert(mt_rand(pow(2, 16), pow(2, 32)), 10, 32);
}

$b = 100;
$t = microtime(true);
for ($i = 0; $i<=$b; $i++) {
