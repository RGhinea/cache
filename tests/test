    );

    unset($cache['key1'], $cache['key2'], $cache['key3'], $cache['key4']);
    unset($cache['key5'], $cache['key6'], $cache['key7'], $cache['key8']);
    unset($cache['key9'], $cache['key10'], $cache['key11'], $cache['key12']);
    unset($cache['key13'], $cache['key14'], $cache['key15'], $cache['key16']);
}
$t = microtime(true)-$t;
var_dump(1000*$t/$b, $t);
die;
